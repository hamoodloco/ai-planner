    "@types/node": "^22.13.11",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "googleapis": "^148.0.0",
    "helmet": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^6.1.0",
    "p-retry": "^6.2.1",
    "prisma": "^6.1.0",
    "tesseract.js": "^5.1.1",
    "zod": "^3.24.1",
    "winston": "^3.17.0",
    "express-rate-limit": "^7.5.0",
